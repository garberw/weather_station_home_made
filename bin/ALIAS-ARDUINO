alias _wjw='SYSTEMD_COLORS=true journalctl -b -u weewx_wmod.service'
alias _wjt='SYSTEMD_COLORS=true journalctl -b -u weewx_timer.timer'
alias _wjE='SYSTEMD_COLORS=true journalctl -b -u weathere.service'
alias _wjG='SYSTEMD_COLORS=true journalctl -b -u weatherg.service'
alias _wjS='SYSTEMD_COLORS=true journalctl -b -u weathers.service'

alias _wfa='SYSTEMD_COLORS=true journalctl -b -u weewx_atlas.service  -f'
alias _wfg='SYSTEMD_COLORS=true journalctl -b -u weewx_green.service  -f'
alias _wfw='SYSTEMD_COLORS=true journalctl -b -u weewx_wmod.service   -f'
alias _wft='SYSTEMD_COLORS=true journalctl -b -u weewx_timer.timer    -f'
alias _wfE='SYSTEMD_COLORS=true journalctl -b -u weathere.service     -f'
alias _wfG='SYSTEMD_COLORS=true journalctl -b -u weatherg.service     -f'
alias _wfS='SYSTEMD_COLORS=true journalctl -b -u weathers.service     -f'

alias _wBa='_wba; _wfa'
alias _wBg='_wbg; _wfg'
alias _wBw='_wbw; _wfw'
alias _wBE='_wbE; _wfE'
alias _wBG='_wbG; _wfG'
alias _wBS='_wbS; _wfS'

alias _wFa='_wea; _wba; _wfa;'
alias _wFg='_weg; _wbg; _wfg;'
alias _wFw='_wew; _wbw; _wfw;'
alias _wFt='_wet; _wbt; _wft;'
alias _wFE='_weE; _wbE; _wfE;'
alias _wFG='_weG; _wbG; _wfG;'
alias _wFS='_weS; _wbS; _wfS;'

