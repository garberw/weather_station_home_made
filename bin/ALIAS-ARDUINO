alias wpc='cat /dev/ttyACMwc'
alias wpl='cat /dev/ttyACMwl'
alias wpr='cat /dev/ttyACMwr'
alias wpw='cat /dev/ttyACMww'
alias wpa='cat /dev/ttyACMwa'
alias wpg='cat /dev/ttyACMwg'

alias WPC-LOG='wpc 2>&1 | tee wpc.`date -Is`.log'
alias WPL-LOG='wpl 2>&1 | tee wpl.`date -Is`.log'
alias WPR-LOG='wpr 2>&1 | tee wpr.`date -Is`.log'
alias WPW-LOG='wpw 2>&1 | tee wpw.`date -Is`.log'
alias WPA-LOG='wpa 2>&1 | tee wpa.`date -Is`.log'
alias WPG-LOG='wpg 2>&1 | tee wpg.`date -Is`.log'

alias _wEa='systemctl enable --now weewx_atlas.service'
alias _wEg='systemctl enable --now weewx_green.service'
alias _wEw='systemctl enable --now weewx_wmod.service'
alias _wEt='systemctl enable --now weewx_timer.timer'
alias _wEx='systemctl enable --now weewx_atlas.service weewx_green.service weewx_wmod.service weewx_timer.timer'
alias _wEE='systemctl enable --now weathere.service'
alias _wEG='systemctl enable --now weatherg.service'
alias _wES='systemctl enable --now weathers.service'

alias _wDa='systemctl disable --now weewx_atlas.service'
alias _wDg='systemctl disable --now weewx_green.service'
alias _wDw='systemctl disable --now weewx_wmod.service'
alias _wDt='systemctl disable --now weewx_timer.timer'
alias _wDx='systemctl disable --now weewx_atlas.service weewx_green.service weewx_wmod.service weewx_timer.timer'
alias _wDE='systemctl disable --now weathere.service'
alias _wDG='systemctl disable --now weatherg.service'
alias _wDS='systemctl disable --now weathers.service'

alias _wba='systemctl start weewx_atlas.service'
alias _wbg='systemctl start weewx_green.service'
alias _wbw='systemctl start weewx_wmod.service'
alias _wbt='systemctl start weewx_timer.timer'
alias _wbx='systemctl start weewx_atlas.service weewx_green.service weewx_wmod.service weewx_timer.timer'
alias _wbE='systemctl start weathere.service'
alias _wbG='systemctl start weatherg.service'
alias _wbS='systemctl start weathers.service'

alias _wea='systemctl stop weewx_atlas.service'
alias _weg='systemctl stop weewx_green.service'
alias _wew='systemctl stop weewx_wmod.service'
alias _wet='systemctl stop weewx_timer.timer'
alias _wex='systemctl stop weewx_atlas.service weewx_green.service weewx_wmod.service weewx_timer.timer'
alias _weE='systemctl stop weathere.service'
alias _weG='systemctl stop weatherg.service'
alias _weS='systemctl stop weathers.service'

alias _wsa='systemctl status weewx_atlas.service'
alias _wsg='systemctl status weewx_green.service'
alias _wsw='systemctl status weewx_wmod.service'
alias _wst='systemctl status weewx_timer.timer'
alias _wsx='systemctl status weewx_atlas.service weewx_green.service weewx_wmod.service weewx_timer.timer'
alias _wsE='systemctl status weathere.service'
alias _wsG='systemctl status weatherg.service'
alias _wsS='systemctl status weathers.service'

alias _wja='SYSTEMD_COLORS=true journalctl -b -u weewx_atlas.service'
